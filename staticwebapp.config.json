{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "no-store"
      }
    }
  ],

  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css",
    ".mp4": "video/mp4",
    ".png": " "
  }
}
